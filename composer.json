{
    "name": "acvos/bubbles",
    "type": "library",
    "description": "Bubbles DI Container",
    "keywords": ["dependency", "injection", "DI", "container", "configuration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Chernikov",
            "email": "achernikov@acvos.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phing/phing": "2.*",
        "phploc/phploc": "2.*@dev",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "1.4.*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-0": { "Acvos\\": "src/" }
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    }
}
